{% block content %}

  <ul>
    {% for i, videoName in videoFiles %}
      <li>
        <a class="btn btn-primary"
           href="{{ path('star-video', {'videoId': i}) }}">
          {{ videoName }}
        </a>
      </li>
    {% endfor %}
  </ul>

{% endblock %}
