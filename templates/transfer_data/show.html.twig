{% extends 'base.html.twig' %}

{% block title %}TransferData{% endblock %}

{% block body %}
    <h1>TransferData</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ transfer_datum.id }}</td>
            </tr>
            <tr>
                <th>FileName</th>
                <td>{{ transfer_datum.fileName }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ transfer_datum.link }}</td>
            </tr>
            <tr>
